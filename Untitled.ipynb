    "def get_middle(s):\n",
    "        i = len(s)\n",
    "        j = i // 2\n",
    "        print(i)\n",
    "        print(j)\n",
    "        if i <= 2:\n",
    "            return s\n",
    "        elif i % 2 == 0:\n",
    "            return s[j:(j + 1)]\n",
    "        elif i % 2 != 0:\n",
    "            return s[j]\n",
    "\n",
    "get_middle(\"test\")"
   ]
